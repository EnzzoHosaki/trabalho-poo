<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.trabalhoPoo.controller.AtividadeController"
            prefHeight="300.0" prefWidth="400.0">
    <VBox spacing="10" AnchorPane.topAnchor="20" AnchorPane.leftAnchor="20" AnchorPane.rightAnchor="20">
        <Label fx:id="nomeLabel" text="Nome: "/>
        <Label fx:id="descricaoLabel" text="Descrição: "/>
        <Label fx:id="dataInicioLabel" text="Data de Início: "/>
        <Label fx:id="dataTerminoLabel" text="Data de Término: "/>
        <Label fx:id="statusLabel" text="Status: "/>
        <Label fx:id="porcentagemLabel" text="Progresso: "/>
        <Label fx:id="justificativaLabel" text="Justificativa: "/>
    </VBox>
</AnchorPane>